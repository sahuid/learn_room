<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sahuid.learnroom.mapper.MessageMapper">

    <resultMap id="BaseResultMap" type="com.sahuid.learnroom.model.entity.Message">
            <id property="messageId" column="messageId" />
            <result property="content" column="content" />
            <result property="status" column="status" />
            <result property="messageType" column="messageType" />
            <result property="createTime" column="createTime" />
    </resultMap>

    <sql id="Base_Column_List">
        messageId,content,status,messageType,createTime
    </sql>
</mapper>
